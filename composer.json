{
  "name": "con4gis/visualization",
  "type": "contao-bundle",
  "description": "Easily visualize your data in different types of charts.",
  "keywords": ["con4gis", "contao", "c4g", "chars", "line chart", "pie chart", "bar chart", "d3js", "c3js"],
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Kuestenschmiede GmbH Software & Design",
      "homepage": "https://kuestenschmiede.de",
      "role": "Operator"
    },
    {
      "name": "con4gis community",
      "homepage": "https://con4gis.org",
      "role": "Developer"
    }
  ],
  "require": {
    "contao/core-bundle": "^4.9",
    "con4gis/core": "^5.1.0",
    "menatwork/contao-multicolumnwizard-bundle": "^3.5.2"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\VisualizationBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "support": {
    "issues": "https://github.com/Kuestenschmiede/VisualizationBundle/issues",
    "email": "support@con4gis.org"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\VisualizationBundle\\ContaoManager\\Plugin"
  }
}